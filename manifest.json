{
  "manifest_version": 3,
  "name": "URLock",
  "version": "1.0.0",
  "description": "A Chrome extension for URL pattern redirection",
  "permissions": [
    "storage",
    "tabs",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "1024": "icons/icon1024.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "1024": "icons/icon1024.png"
  }
} 